    "@aws-sdk/client-sqs": "^3.473.0",
    "@aws-sdk/lib-storage": "^3.472.0",
    "@aws-sdk/util-dynamodb": "^3.473.0",
    "@types/cheerio": "^0.22.35",
    "aws-cdk-lib": "^2.116.0",
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.116.0-alpha.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "constructs": "^10.0.0",
    "html-to-text": "^9.0.5",
