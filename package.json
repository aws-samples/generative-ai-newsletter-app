{
  "type": "commonjs",
  "name": "genai-newsletter",
  "version": "1.1.15",
  "bin": {
    "genai-newsletter-app": "bin/genai-newsletter-app.js"
  },
  "scripts": {
    "config": "node -r ts-node/register ./cli/config.ts",
    "build": "tsc && npm run codegen && npm run build:appsync && npm run build:ui",
    "build:ui": "cd lib/user-interface/genai-newsletter-ui/ && vite build",
    "build:appsync": "ts-node ./lib/api/functions/bundle.ts",
    "watch": "tsc -w",
    "lint": "eslint . --fix --ext .ts,.tsx --report-unused-disable-directives",
    "prettier": "prettier --write \"**/*.{ts,tsx}\"",
    "format": "npm run prettier && npm run lint",
    "test": "jest",
    "deploy": "cdk deploy",
    "cdk": "cdk",
    "commit": "cz",
    "codegen": "npm run codegen:api && npm run codegen:auth",
    "codegen:api": "cd lib/shared/api && npx @aws-amplify/cli codegen",
    "codegen:auth": "cd lib/shared/api/schema-to-avp && npx graphql-codegen ./codegen.ts",
    "email:start": "cd lib/shared/email-generator && npm run start",
    "ui:start": "cd lib/user-interface/genai-newsletter-ui/ && vite",
    "docs:dev": "vitepress dev pages",
    "docs:build": "vitepress build pages",
    "docs:preview": "vitepress preview pages"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^12.10.1",
    "@aws-appsync/utils": "^1.7.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.554.0",
    "@aws-sdk/client-lambda": "^3.496.0",
    "@aws-sdk/client-s3": "^3.472.0",
    "@aws-sdk/client-verifiedpermissions": "^3.515.0",
    "@cloudscape-design/components": "^3.0.593",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/plugin-helpers": "^5.0.3",
    "@middy/core": "^5.2.1",
    "@react-email/components": "0.0.12",
    "@react-email/markdown": "^0.0.8",
    "@react-email/render": "^0.0.10",
    "@types/ansi-escape-sequences": "^4.0.4",
    "@types/aws-lambda": "^8.10.130",
    "@types/figlet": "^1.5.8",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "20.9.0",
    "@types/prompt-sync": "^4.2.3",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "ansi-escape-sequences": "^6.2.2",
    "aws-amplify": "^6.0.9",
    "aws-cdk": "^2.135.0",
    "commitizen": "^4.3.0",
    "compare-versions": "^6.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "figlet": "^1.7.0",
    "git-conventional-commits": "^2.6.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prompt-sync": "^4.2.0",
    "react-email": "^1.10.0",
    "react-router-dom": "^6.22.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5",
    "vite": "^5.0.12",
    "vitepress": "^1.1.4"
  },
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.9.1",
    "@aws-amplify/ui-react": "^6.1.6",
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.134.0-alpha.0",
    "@aws-lambda-powertools/logger": "^1.17.0",
    "@aws-lambda-powertools/metrics": "^1.17.0",
    "@aws-lambda-powertools/tracer": "^2.1.0",
    "@aws-sdk/client-appsync": "^3.556.0",
    "@aws-sdk/client-bedrock-runtime": "^3.474.0",
    "@aws-sdk/client-dynamodb": "^3.473.0",
    "@aws-sdk/client-pinpoint": "^3.478.0",
    "@aws-sdk/client-scheduler": "^3.478.0",
    "@aws-sdk/client-sfn": "^3.569.0",
    "@aws-sdk/client-ssm": "^3.519.0",
    "@aws-sdk/lib-storage": "^3.472.0",
    "@aws-sdk/util-dynamodb": "^3.473.0",
    "@cloudscape-design/global-styles": "^1.0.26",
    "@types/cheerio": "^0.22.35",
    "aws-cdk-lib": "^2.134.0",
    "aws-jwt-verify": "^4.0.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.2",
    "cdk-nag": "^2.28.109",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "constructs": "^10.0.0",
    "cz": "^1.8.2",
    "graphql": "^16.8.1",
    "mui-color-input": "^2.0.3",
    "source-map-support": "^0.5.21"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
